long long penwick_tree[size+1];
long long arr[size+1];
void update(int x, long long dif) {
    while (x <= size) {
        data[x] += dif;
    	x += (x&-x);
    }
}
long long sum(int x) {
    long long ret = 0;
    while (x) {
        ret += penwick_tree[x];
        x &= x - 1;
    }
    return ret;
}
long long sum(int x, int y) {
    return sum(y) - sum(x - 1);
}
